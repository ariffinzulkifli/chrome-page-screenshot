{
    "manifest_version": 3,
    "name": "MyWebCapture",
    "description": "Full Web Page Page Screenshot",
    "version": "0.1",
    "permissions": [
        "activeTab", "scripting"
    ],
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "MyCapture",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ]
}